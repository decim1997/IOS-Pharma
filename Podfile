# platform :ios, '9.0'

target 'Pharma+' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'GoogleSignIn'
  pod 'Google/SignIn'


  # Pods for Pharma+

  target 'Pharma+Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Pharma+UITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
